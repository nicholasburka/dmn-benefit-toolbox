<?xml version="1.0" encoding="UTF-8"?>
<dmn:definitions
  xmlns:dmn="http://www.omg.org/spec/DMN/20180521/MODEL/"
  xmlns="https://kie.apache.org/dmn/_F6A7B8C9-0D1E-2F3A-4B5C-6D7E8F9A0B1C"
  xmlns:feel="http://www.omg.org/spec/DMN/20180521/FEEL/"
  xmlns:kie="http://www.drools.org/kie/dmn/1.2"
  id="_D2C1B0E9-8F7A-6E5B-4D3C-2E1F0A9B8C7D"
  name="ParoledAlien"
  typeLanguage="http://www.omg.org/spec/DMN/20180521/FEEL/"
  namespace="https://kie.apache.org/dmn/_F6A7B8C9-0D1E-2F3A-4B5C-6D7E8F9A0B1C">

  <dmn:description>Paroled alien per INA Section 212(d)(5) for at least 1 year. Qualified alien status. AR Code: G</dmn:description>

  <dmn:import id="_A7B8C9D0" name="BDT" namespace="https://kie.apache.org/dmn/_1B91A885-130A-4E0B-A762-E12AA6DD5C79" locationURI="../../bdt.dmn" importType="http://www.omg.org/spec/DMN/20180521/MODEL/"/>

  <dmn:itemDefinition id="_B8C9D0E1" name="tParameters" isCollection="false">
    <dmn:itemComponent id="_C9D0E1F2" name="personId"><dmn:typeRef>string</dmn:typeRef></dmn:itemComponent>
  </dmn:itemDefinition>

  <dmn:decisionService id="_D0E1F2A3" name="ParoledAlienService">
    <dmn:variable id="_E1F2A3B4" name="ParoledAlienService" typeRef="BDT.tCheckResponse"/>
    <dmn:outputDecision href="#_F2A3B4C5"/>
    <dmn:inputData href="#_A3B4C5D6"/>
    <dmn:inputData href="#_B4C5D6E7"/>
  </dmn:decisionService>

  <dmn:inputData id="_B4C5D6E7" name="situation">
    <dmn:variable id="_C5D6E7F8" name="situation" typeRef="BDT.tSituation"/>
  </dmn:inputData>

  <dmn:inputData id="_A3B4C5D6" name="parameters">
    <dmn:variable id="_D6E7F8A9" name="parameters" typeRef="tParameters"/>
  </dmn:inputData>

  <dmn:decision id="_F2A3B4C5" name="checkResult">
    <dmn:variable id="_E7F8A9B0" name="checkResult" typeRef="boolean"/>
    <dmn:informationRequirement id="_F8A9B0C1"><dmn:requiredInput href="#_B4C5D6E7"/></dmn:informationRequirement>
    <dmn:informationRequirement id="_A9B0C1D2"><dmn:requiredInput href="#_A3B4C5D6"/></dmn:informationRequirement>
    <dmn:context id="_B0C1D2E3">
      <dmn:contextEntry>
        <dmn:variable id="_C1D2E3F4" name="person" typeRef="BDT.tPerson"/>
        <dmn:literalExpression id="_D2E3F4A5"><dmn:text>situation.people[id = parameters.personId][1]</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
      <dmn:contextEntry>
        <dmn:variable id="_E3F4A5B6" name="result" typeRef="boolean"/>
        <dmn:literalExpression id="_F4A5B6C7"><dmn:text>person.citizenshipStatus = "PAROLED_ALIEN"</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
      <dmn:contextEntry>
        <dmn:literalExpression id="_A5B6C7D8"><dmn:text>result</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
    </dmn:context>
  </dmn:decision>
</dmn:definitions>
