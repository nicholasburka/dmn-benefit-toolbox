<?xml version="1.0" encoding="UTF-8"?>
<dmn:definitions
  xmlns:dmn="http://www.omg.org/spec/DMN/20180521/MODEL/"
  xmlns="https://kie.apache.org/dmn/_E5F6A7B8-9C0D-1E2F-3A4B-5C6D7E8F9A0B"
  xmlns:feel="http://www.omg.org/spec/DMN/20180521/FEEL/"
  xmlns:kie="http://www.drools.org/kie/dmn/1.2"
  id="_C1B0A9D8-7E6F-5A4B-3C2D-1E0F9A8B7C6D"
  name="CubanHaitianEntrant"
  typeLanguage="http://www.omg.org/spec/DMN/20180521/FEEL/"
  namespace="https://kie.apache.org/dmn/_E5F6A7B8-9C0D-1E2F-3A4B-5C6D7E8F9A0B">

  <dmn:description>Cuban/Haitian entrant per Section 501(e) Refugee Education Assistance Act 1980. Qualified alien with 7-year SSI limit.</dmn:description>

  <dmn:import id="_F6A7B8C9" name="BDT" namespace="https://kie.apache.org/dmn/_1B91A885-130A-4E0B-A762-E12AA6DD5C79" locationURI="../../bdt.dmn" importType="http://www.omg.org/spec/DMN/20180521/MODEL/"/>

  <dmn:itemDefinition id="_A7B8C9D0" name="tParameters" isCollection="false">
    <dmn:itemComponent id="_B8C9D0E1" name="personId"><dmn:typeRef>string</dmn:typeRef></dmn:itemComponent>
  </dmn:itemDefinition>

  <dmn:decisionService id="_C9D0E1F2" name="CubanHaitianEntrantService">
    <dmn:variable id="_D0E1F2A3" name="CubanHaitianEntrantService" typeRef="BDT.tCheckResponse"/>
    <dmn:outputDecision href="#_E1F2A3B4"/>
    <dmn:inputData href="#_F2A3B4C5"/>
    <dmn:inputData href="#_A3B4C5D6"/>
  </dmn:decisionService>

  <dmn:inputData id="_A3B4C5D6" name="situation">
    <dmn:variable id="_B4C5D6E7" name="situation" typeRef="BDT.tSituation"/>
  </dmn:inputData>

  <dmn:inputData id="_F2A3B4C5" name="parameters">
    <dmn:variable id="_C5D6E7F8" name="parameters" typeRef="tParameters"/>
  </dmn:inputData>

  <dmn:decision id="_E1F2A3B4" name="checkResult">
    <dmn:variable id="_D6E7F8A9" name="checkResult" typeRef="boolean"/>
    <dmn:informationRequirement id="_E7F8A9B0"><dmn:requiredInput href="#_A3B4C5D6"/></dmn:informationRequirement>
    <dmn:informationRequirement id="_F8A9B0C1"><dmn:requiredInput href="#_F2A3B4C5"/></dmn:informationRequirement>
    <dmn:context id="_A9B0C1D2">
      <dmn:contextEntry>
        <dmn:variable id="_B0C1D2E3" name="person" typeRef="BDT.tPerson"/>
        <dmn:literalExpression id="_C1D2E3F4"><dmn:text>situation.people[id = parameters.personId][1]</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
      <dmn:contextEntry>
        <dmn:variable id="_D2E3F4A5" name="result" typeRef="boolean"/>
        <dmn:literalExpression id="_E3F4A5B6"><dmn:text>person.citizenshipStatus = "CUBAN_HAITIAN_ENTRANT"</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
      <dmn:contextEntry>
        <dmn:literalExpression id="_F4A5B6C7"><dmn:text>result</dmn:text></dmn:literalExpression>
      </dmn:contextEntry>
    </dmn:context>
  </dmn:decision>
</dmn:definitions>
